<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.browser.jammukashmir"
    android:background="@drawable/animationkashmir"
    android:id="@+id/layout6">

    <Button
        android:id="@+id/button16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="About jammu and kashmir"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:onClick="about5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        android:layout_marginBottom="16dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="109dp"
        android:layout_height="46dp"
        android:layout_marginBottom="27dp"
        android:text="HOTEL"
        app:layout_constraintBottom_toTopOf="@+id/button16"
        app:layout_constraintLeft_toLeftOf="parent"
        android:onClick="hotel1"
        app:layout_constraintHorizontal_bias="0.926"
        app:layout_constraintRight_toLeftOf="@+id/button8"
        android:layout_marginRight="8dp"
        />

    <Button
        android:id="@+id/button7"
        android:layout_width="98dp"
        android:layout_height="46dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="8dp"
        android:text="places"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/button6"
        android:onClick="spots2"
        app:layout_constraintTop_toTopOf="@+id/button6"
        android:layout_marginTop="0dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="102dp"
        android:layout_height="44dp"
        android:text="Food"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/button7"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="1.0"
        android:onClick="food2"
        app:layout_constraintTop_toTopOf="@+id/button6"
        android:layout_marginTop="0dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="250dp"
        android:layout_height="43dp"
        android:layout_marginBottom="300dp"
        android:text="CHECK OUT JAMMU AND KASHMIR"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/button6"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />
</android.support.constraint.ConstraintLayout>
